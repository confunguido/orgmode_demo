@article{Ferguson2006,
  title = {Strategies for Mitigating an Influenza Pandemic},
  volume = {442},
  issn = {0028-0836},
  doi = {10.1038/nature04795},
  abstract = {Development of strategies for mitigating the severity of a new\textbackslash{}n\textbackslash{}ninfluenza pandemic is now a top global public health priority.\textbackslash{}n\textbackslash{}nInfluenza prevention and containment strategies can be con-\textbackslash{}n\textbackslash{}nsidered under the broad categories of antiviral, vaccine and non-\textbackslash{}n\textbackslash{}npharmaceutical (case isolation, household quarantine, school or\textbackslash{}n\textbackslash{}nworkplace closure, restrictions on travel) measures1. Mathematical\textbackslash{}n\textbackslash{}nmodels are powerful tools for exploring this complex landscape of\textbackslash{}n\textbackslash{}nintervention strategies and quantifying the potential costs and\textbackslash{}n\textbackslash{}nbenefits of different options2\textendash{}5. Here we use a large-scale epidemic\textbackslash{}n\textbackslash{}nsimulation6 to examine intervention options should initial con-\textbackslash{}n\textbackslash{}ntainment6,7 of a novel influenza outbreak fail, using Great Britain\textbackslash{}n\textbackslash{}nand the United States as examples. We find that border restrictions\textbackslash{}n\textbackslash{}nand/or internal travel restrictions are unlikely to delay spread by\textbackslash{}n\textbackslash{}nmore than 2\textendash{}3 weeks unless more than 99\% effective. School\textbackslash{}n\textbackslash{}nclosure during the peak of a pandemic can reduce peak attack\textbackslash{}n\textbackslash{}nrates by up to 40\%, but has little impact on overall attack rates,\textbackslash{}n\textbackslash{}nwhereas case isolation or household quarantine could have a\textbackslash{}n\textbackslash{}nsignificant impact, if feasible. Treatment of clinical cases can\textbackslash{}n\textbackslash{}nreduce transmission, but only if antivirals are given within a day\textbackslash{}n\textbackslash{}nof symptoms starting. Given enough drugs for 50\% of the\textbackslash{}n\textbackslash{}npopulation, household-based prophylaxis coupled with reactive\textbackslash{}n\textbackslash{}nschool closure could reduce clinical attack rates by 40\textendash{}50\%. More\textbackslash{}n\textbackslash{}nwidespread prophylaxis would be even more logistically challeng-\textbackslash{}n\textbackslash{}ning but might reduce attack rates by over 75\%. Vaccine stockpiled\textbackslash{}n\textbackslash{}nin advance of a pandemic could significantly reduce attack\textbackslash{}n\textbackslash{}nrates even if of low efficacy. Estimates of policy effectiveness will\textbackslash{}n\textbackslash{}nchange if the characteristics of a future pandemic strain differ\textbackslash{}n\textbackslash{}nsubstantially from those seen in past pandemics.},
  number = {7101},
  journal = {Nature},
  author = {Ferguson, Neil M and Cummings, Derek A T and Fraser, Christophe and Cajka, James C and Cooley, Philip C and Burke, Donald S},
  year = {2006},
  keywords = {antivirals,h5n1,influenza,mathematical model,neuraminidase inhibitor,pandemic},
  pages = {448-452},
  isbn = {1476-4687 (Electronic)\textbackslash{}r0028-0836 (Linking)},
